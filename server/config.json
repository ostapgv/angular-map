{
  "restApiExplorer": "/explorer",
  "host": "0.0.0.0",
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
      "origin": true,
      "credentials": true
    },
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "index": "/index.html",
  "email": {
    "admin": "gpoatg@gmail.com",
    "support": "gpoatg@gmail.com"
  }
}
